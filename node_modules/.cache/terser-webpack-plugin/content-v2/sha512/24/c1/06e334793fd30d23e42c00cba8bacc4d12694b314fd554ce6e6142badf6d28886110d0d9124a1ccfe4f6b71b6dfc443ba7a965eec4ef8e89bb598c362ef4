         errors.name = 'Invalid name';\n          }\n          if (values.phone && !/\\d{9}/.test(values.phone)) {\n            errors.phone = 'Invalid number e.g. 750200100';\n          }\n          if (!values.date) {\n            errors.date = 'Select date appointment';\n          }\n          if (!values.time) {\n            errors.time = 'Select time appointment';\n          }\n          if (!values.guest) {\n            errors.guest = 'Select your table';\n          }\n\n          return errors;\n        }}\n        onSubmit={(values, { setSubmitting }) => {\n          setTimeout(() => {\n            const date = values.date.split(/\\D/g);\n            const newFormatDate = [date[2], date[1], date[0]].join('-');\n            values.date = newFormatDate;\n            setContext(values);\n          }, 400);\n        }}\n      >\n        {({\n          values,\n          errors,\n          touched,\n          handleChange,\n          handleBlur,\n          handleSubmit,\n          isSubmitting,\n        }) => (\n          <form onSubmit={handleSubmit} className={firstStageClasses}>\n            <Container customClass=\"form__input\">\n              <Field\n                type=\"text\"\n                name=\"name\"\n                onChange={handleChange}\n                onBlur={handleBlur}\n                value={values.name}\n                placeholder=\"Name\"\n              />\n              <span className=\"form__validation\">\n                {errors.name && touched.name && errors.name}\n              </span>\n            </Container>\n            <Container customClass=\"form__input\">\n              <Field\n                type=\"email\"\n                name=\"email\"\n                onChange={handleChange}\n                onBlur={handleBlur}\n                value={values.email}\n                placeholder=\"Email\"\n              />\n              <span className=\"form__validation\">\n                {errors.email && touched.email && errors.email}\n              </span>\n            </Container>\n            <Container customClass=\"form__input\">\n              <Field\n                type=\"tel\"\n                name=\"phone\"\n                onChange={handleChange}\n                onBlur={handleBlur}\n                value={values.phone}\n                placeholder=\"Number\"\n              />\n              <span className=\"form__validation\">\n                {errors.phone && touched.phone && errors.phone}\n              </span>\n            </Container>\n\n            <Container customClass=\"form__input\">\n              <Field\n                type=\"date\"\n                name=\"date\"\n                onChange={handleChange}\n                onBlur={handleBlur}\n                value={values.date}\n                min={currentDate}\n                max=\"2022-12-31\"\n              />\n              <span className=\"form__validation\">\n                {errors.date && touched.date && errors.date}\n              </span>\n            </Container>\n\n            <Container customClass=\"form__input\">\n              <label name=\"time\" className=\"form__label\">\n                time\n                <Field component=\"select\" name=\"time\">\n                  <option value=\"\" disabled>\n                    Select time appointment\n                  </option>\n                  <option value=\"12:00pm\">12:00pm</option>\n                  <option value=\"01:00pm\">01:00pm</option>\n                  <option value=\"02:00pm\">02:00pm</option>\n                  <option value=\"03:00pm\">03:00pm</option>\n                  <option value=\"06:00pm\">06:00pm</option>\n                  <option value=\"07:00pm\">07:00pm</option>\n                  <option value=\"08:00pm\">08:00pm</option>\n                  <option value=\"09:00pm\">09:00pm</option>\n                  <option value=\"10:00pm\">10:00pm</option>\n                </Field>\n              </label>\n              <span className=\